<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No.</th>
      <th>Nama Koin</th>
      <th>Jumlah koin</th>
      <th>Harga koin</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i< nominal.length; i++){ %>
    <tr>
      <td><%= i+1%></td>
      <td><%= nominal[i].coinName %></td>
      <td><%= nominal[i].coinQuantity %></td>
      <td><%= nominal[i].price %></td>
      <td>
        <a
          href="/nominal/edit/<%= nominal[i]._id %>"
          class="btn btn-info btn-sm"
        >
          <i class="fas fa-pencil-alt"></i> Ubah
        </a>
        <form
          style="display: inline-block"
          action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE"
          method="POST"
        >
          <button class="btn btn-danger btn-sm">
            <i class="fas fa-trash"></i> Hapus
          </button>
        </form>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
